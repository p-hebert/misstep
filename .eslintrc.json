{
    "env": {
        "es6": true
    },
    "extends": [
      "eslint:recommended",
      "standard"
    ],
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 6,
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true
        }
    },
    "rules": {
      "indent": ["warn", 2],
      "keyword-spacing": ["error", {
        "before": false,
        "after": false,
        "overrides": {
          "case": {"after": true},
          "export": {"after": true},
          "from": {"after": true, "before": true},
          "function": {"after": true, "before": true},
          "import": {"after": true},
          "return": {"after": true}
        }
      }],
      "space-before-blocks": [0, "never"],
      "no-multiple-empty-lines": 0,
      "space-before-function-paren": 0,
      "camelcase": 0,
      "linebreak-style": ["error", "unix"],
      "semi": ["error", "always"],
      "no-unused-vars": ["warn"],
      "no-console": "off"
    }
}
